<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane maxHeight="200.0" maxWidth="400.0"  xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.login.loginControler">
   <children>
      <Button layoutX="175.0" layoutY="136.0" mnemonicParsing="false" fx:id="loginButton" text="Log In" onAction="#logIn" />
      <Button layoutX="171.0" layoutY="167.0" mnemonicParsing="false" fx:id="signupButton" text="Sign Up" onAction="#signUp" />
      <TextField layoutX="126.0" layoutY="45.0" fx:id="emailField" promptText="examplemail@mail.com" />
      <TextField layoutX="126.0" layoutY="88.0" fx:id="passwordField" promptText="pasword12" />
      <Label layoutX="71.0" layoutY="49.0" fx:id="emailLabel" text="Email" />
      <Label layoutX="60.0" layoutY="92.0" fx:id="passwordLabel" text="Password" />
   </children>
</AnchorPane>
